prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix : <https://example.net/terms/>
base <https://example.org/>

graph <using-qualification> {

  <illustration-activity> a prov:Activity;
    prov:usageQualification [ a prov:Usage;
          prov:entity <aggregated-by-regions>;
          prov:atTime "2011-07-14T03:03:03Z"^^xsd:dateTime
      ] .

  <painting> :height [ rdf:value 32.1 ;
        :unit <cm>;
        :measurementTechnique <laser-scanning>;
        :measuredOn "2020-02-11"^^xsd:date
    ] .

}

graph <using-annotation> {

  <illustration-activity> a prov:Activity;
    prov:used <aggregated-by-regions> {|
          prov:atTime "2011-07-14T03:03:03Z"^^xsd:dateTime
      |} .

  <painting> :height 32.1 {|
        :unit <cm>;
        :measurementTechnique <laser-scanning>;
        :measuredOn "2020-02-11"^^xsd:date
    |} .

}
